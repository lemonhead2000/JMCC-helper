ommand":"jmcc.runActiveFile","key":"F5","when":"editorTextFocus"}]},"scripts":{"vscode:prepublish":"npm run compile","compile":"tsc -p ./tsconfig.json && npm run copy-py && npm run copy-deps","copy-py":"copyfiles -u 1 \"src/*.py\" out/ \"src/assets/**\" out/ || xcopy /Y src\\*.py out\\ && xcopy /E /Y src\\assets out\\assets 2> nul || cp src/*.py out/ && cp -r src/assets out/ 2> nul","copy-deps":"copyfiles -u 1 \"node_modules/{balanced-match,vscode-jsonrpc,vscode-languageclient,vscode-languageserver-protocol,vscode-languageserver-types}/**\" out/node_modules/ && copyfiles -u 1 \"node_modules/{balanced-match,vscode-jsonrpc,vscode-languageclient,vscode-languageserver-protocol,vscode-languageserver-types}/package.json\" out/node_modules/","package":"npm run compile && npx vsce package","watch":"tsc -watch -p ./tsconfig.json","postinstall":"npm run copy-py"},"dependencies":{"vscode-languageclient":"^8.1.0"},"devDependencies":{"@types/node":"^24.1.0","mocha":"^10.0.0","mkdirp":"^1.0.4","typescript":"^4.5.0","vscode":"^1.1.37","copyfiles":"^2.4.1"}}